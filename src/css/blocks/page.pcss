ul {
  padding: 0;
  list-style-type: none;
}

.page,
.page__body {
  height: 100%;
}

.page {
  color: var(--color-text-primary);
  font-family: var(--default-font);
  background-color: var(--dark);
  scroll-behavior: smooth;
}

.page__body {
  max-width: 1920px;
  margin: 0 auto;
  font-size: 16px;
}
